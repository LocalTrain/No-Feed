{
  "manifest_version": 2,

  "name": "No Feed",
  "description": "This extension hides your Facebook news feed inturn increases your productivity.",
  "version": "0.1.0",

  "content_scripts": [
    {
  	  "matches" : ["*://*.facebook.com/", "*://*.facebook.com/*ref*tnmn/"],
	  "css": ["removefeed.css"]
	}
   ],

  "browser_action": {
    "default_icon": "code.png",
    "default_popup": "index.html"
  }
}
